{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C#: DestinyTrail.Blazor Debug",
            "type": "dotnet",
            "request": "launch",
            "projectPath": "${workspaceFolder}/DestinyTrail.Blazor/DestinyTrail.Blazor.csproj"
        },


        {
            "name": "DEBUG console",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DebugConsole/bin/Debug/net8.0/DebugConsole.exe",
            "args": [],
            "cwd": "${workspaceFolder}/DebugConsole/bin/Debug/net8.0",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        },
        {
            "name": "Avalonia DEBUG",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/DestinyTrail/bin/Debug/net8.0/DestinyTrail.exe",
            "args": [],
            "cwd": "${workspaceFolder}/DestinyTrail/bin/Debug/net8.0",
            "stopAtEntry": false,
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            }
        }
    ]
}
